public class Main {

    public static void main(String[] args) {

       //Scanner sc= new Scanner(System.in);
       int a= 2;
       int b=3;
       int c=99;
       int d=90;

       b+= 7;
       a *=2;
       c /=3;
       d -=10;


        System.out.println(a);
        System.out.println(b);
        System.out.println(c);
        System.out.println(d);
    }}
